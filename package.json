{
  "name": "gradient-blur",
  "version": "1.0.0",
  "description": "Your Figma Plugin",
  "main": "code.js",
  "scripts": {
    "build": "build-figma-plugin --typecheck --minify",
    "dev": "build-figma-plugin --typecheck --watch",
    "lint": "eslint --ext .ts,.tsx --ignore-pattern node_modules .",
    "lint:fix": "eslint --ext .ts,.tsx --ignore-pattern node_modules --fix ."
    },
  "author": "",
  "license": "",
  "devDependencies": {
    "@create-figma-plugin/build": "^4.0.0-alpha.0",
    "@create-figma-plugin/tsconfig": "^4.0.0-alpha.0",
    "@figma/eslint-plugin-figma-plugins": "*",
    "@figma/plugin-typings": "^1.108.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "typescript": "^5.3.2"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:@figma/figma-plugins/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "root": true,
    "rules": {
      "@typescript-eslint/no-unused-vars": [
        "error",
        {
          "argsIgnorePattern": "^_",
          "varsIgnorePattern": "^_",
          "caughtErrorsIgnorePattern": "^_"
        }
      ]
    }
  },
  "dependencies": {
    "@create-figma-plugin/ui": "^4.0.0-alpha.0",
    "@create-figma-plugin/utilities": "^4.0.0-alpha.0"
  },
  "figma-plugin": {
    "editorType": [
      "figma"
    ],
    "name": "Rectangles",
    "main": "src/controller.ts",
    "ui": "src/ui.tsx"
  }
}
